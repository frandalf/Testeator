<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_eie"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.frandalf.testeator1evaluacin.EIE">

    <Button
        android:text="UNIDAD 2 - TEST"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/BEIE_U1"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="unidad 2 - CUESTIONARIO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BEIE_U1"
        android:layout_alignParentStart="true"
        android:id="@+id/BEIE_U2"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="UNIDAD 3 - CUESTIONARIO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BEIE_U3"
        android:layout_alignParentStart="true"
        android:id="@+id/BEIE_U4"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="UNIDAD 4 - CUESTIONARIO"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/BEIE_U5"
        android:layout_below="@+id/BEIE_U4"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="UNIDAD 3 - TEST"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/BEIE_U3"
        android:layout_below="@+id/BEIE_U2"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Cuestionario 1º Evaluación"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/BEIE_U5"
        android:layout_alignParentStart="true"
        android:id="@+id/BEIE_Final"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
